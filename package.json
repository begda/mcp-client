{
  "name": "@begda/mcp-client",
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "keywords": [
    "echo",
    "begda"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com/begda/mcp_cli.git"
  },
  "homepage": "https://blog.tianliaohui.com",
  "publishConfig": {
    "tag": "latest",
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "npm-撤回上次发布的包-仅限24小时内": "npm unpublish @begda/echo --force",
    "npm-查看服务器版本号": "npm view @begda/echo versions",
    "npm-发布:第一次": "npm publish",
    "npm-发布:打补丁": "npm version patch && npm publish",
    "npm-发布:小改": "npm version minor && npm publish",
    "npm-发布:重大更新": "npm version major && npm publish"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.9.0",
    "@modelcontextprotocol/server-filesystem": "^2025.3.28",
    "axios": "^1.8.4",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "ollama": "^0.5.14",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "typescript": "^5.5.3",
    "vite": "^6.3.0"
  }
}
